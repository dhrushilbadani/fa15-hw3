<h1>Pages#home</h1>
<h2> Users </h2>
<% @allUsers.each do |u| %>
    <p> <%=u.username%> <%=u.email%> <%=u.age%> </p>
<%end%>

<h2> Cats </h2>
<% @allCats.each do |c| %>
    <p> <%=c.name%> </p>
<%end%>

<h2> Todo </h2>
<% @allTodo.each do |t| %>
    <p> <%=t.tasks%> </p>
<%end%>

<p>Find me in app/views/pages/home.html.erb</p>
